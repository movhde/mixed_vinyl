{% extends 'base.html.twig' %}

{% block title %}Create a new Record | {{ parent() }}{% endblock %}

{% block body %}
<h1>{{ title|upper }}</h1>

<div>
  Tracks:
  <ul>
  {% for track in tracks %}
    <li>
      {{ track.song }} - {{ track.artist }}
    </li>
  {% endfor %}
  </ul>
</div>

{% endblock %}